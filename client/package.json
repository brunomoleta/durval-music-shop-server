    "@radix-ui/react-visually-hidden": "^1.0.3",
    "@types/cors": "^2.8.17",
    "axios": "^1.6.1",
    "focus-trap-react": "^10.2.3",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.3",
    "netlify-cli": "^17.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hook-form": "^7.48.2",
    "react-icons": "^4.12.0",
    "react-router-dom": "^6.18.0",
